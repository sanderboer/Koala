<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net48;net6.0-windows</TargetFrameworks>
    <ProjectGuid>{169DCACE-CAB8-4CDB-9B9A-56310D242078}</ProjectGuid>
    <RootNamespace>Koala</RootNamespace>
    <AssemblyName>Koala</AssemblyName>
    <MyType Condition="'$(TargetFramework)' == 'net48'">Windows</MyType>
    <MyType Condition="'$(TargetFramework)' == 'net6.0-windows'">Empty</MyType>
    <OptionExplicit>On</OptionExplicit>
    <OptionCompare>Binary</OptionCompare>
    <OptionStrict>Off</OptionStrict>
    <OptionInfer>On</OptionInfer>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DocumentationFile>Koala.xml</DocumentationFile>
    <EnableDefaultEmbeddedResourceItems>false</EnableDefaultEmbeddedResourceItems>
    <GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

  <PropertyGroup Condition="'$(TargetFramework)' == 'net6.0-windows' AND '$(OS)' == 'Windows_NT'">
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <PropertyGroup Condition="'$(TargetFramework)' == 'net48' AND '$(OS)' == 'Windows_NT'">
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Debug64' OR '$(Configuration)'=='Debug'">
    <OutputPath Condition="'$(OS)' == 'Windows_NT'">$(APPDATA)\Grasshopper\Libraries\</OutputPath>
    <OutputPath Condition="'$(OS)' != 'Windows_NT'">$(HOME)/.config/Grasshopper/Libraries/</OutputPath>
    <NoWarn>42016,41999,42017,42018,42019,42032,42036,42020,42021,42022</NoWarn>
    <DefineConstants Condition="'$(TargetFramework)' == 'net6.0-windows' AND '$(OS)' == 'Windows_NT'">NET6_0_OR_GREATER</DefineConstants>
    <DefineConstants Condition="'$(TargetFramework)' == 'net6.0-windows' AND '$(OS)' != 'Windows_NT'">NET6_0_OR_GREATER,LINUX</DefineConstants>
    <DefineConstants Condition="'$(TargetFramework)' == 'net48' AND '$(OS)' != 'Windows_NT'">LINUX</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Release64' OR '$(Configuration)'=='Release'">
    <OutputPath>bin\Release\</OutputPath>
    <NoWarn>42016,41999,42017,42018,42019,42032,42036,42020,42021,42022</NoWarn>
    <DefineConstants Condition="'$(TargetFramework)' == 'net6.0-windows' AND '$(OS)' == 'Windows_NT'">NET6_0_OR_GREATER</DefineConstants>
    <DefineConstants Condition="'$(TargetFramework)' == 'net6.0-windows' AND '$(OS)' != 'Windows_NT'">NET6_0_OR_GREATER,LINUX</DefineConstants>
    <DefineConstants Condition="'$(TargetFramework)' == 'net48' AND '$(OS)' != 'Windows_NT'">LINUX</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug64|AnyCPU' OR '$(Configuration)|$(Platform)' == 'Debug|AnyCPU'">
    <StartProgram Condition="'$(OS)' == 'Windows_NT' AND '$(TargetFramework)' == 'net48'">C:\Program Files\Rhino 6\System\Rhino.exe</StartProgram>
    <StartProgram Condition="'$(OS)' == 'Windows_NT' AND '$(TargetFramework)' == 'net6.0-windows'">C:\Program Files\Rhino 8\System\Rhino.exe</StartProgram>
    <StartProgram Condition="'$(OS)' != 'Windows_NT'">/Applications/RhinoWIP.app/Contents/MacOS/Rhino</StartProgram>
    <StartAction>Program</StartAction>
  </PropertyGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net48'">
    <PackageReference Include="Grasshopper" Version="6.34.21034.7001" />
    <PackageReference Include="RhinoCommon" Version="6.34.21034.7001" />
    <PackageReference Include="System.Resources.Extensions" Version="8.0.0" />
